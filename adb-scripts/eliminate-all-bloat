# WARNING: BE VERY CAREFUL WHEN RUNNING THIS SCRIPT!
# Note: You will need adb (Android Debugging Bridge) installed

#!/usr/bin/bash

# Google apps
adb shell pm uninstall -k --user 0 com.google.android.youtube
adb shell pm uninstall -k --user 0 com.google.android.music
adb shell pm uninstall -k --user 0 com.google.android.apps.maps
adb shell pm uninstall -k --user 0 com.google.android.apps.photos
adb shell pm uninstall -k --user 0 com.google.android.apps.tachyon #Google Duo
adb shell pm uninstall -k --user 0 com.google.android.googlequicksearchbox #Google App
adb shell pm uninstall -k --user 0 com.google.android.videos #Google Play Movies (Outdated)
adb shell pm uninstall -k --user 0 com.google.android.talk #Hangouts
adb shell pm uninstall -k --user 0 com.google.android.apps.walletnfcrel #Google Play
adb shell pm uninstall -k --user 0 com.android.chrome
adb shell pm uninstall -k --user 0 com.google.android.gm #Gmail
adb shell pm uninstall -k --user 0 com.google.android.apps.docs #Google Drive
adb shell pm uninstall -k --user 0 com.google.android.apps.docs.editors.docs
adb shell pm uninstall -k --user 0 com.google.android.apps.docs.editors.slides
adb shell pm uninstall -k --user 0 com.google.android.apps.docs.editors.sheets
adb shell pm uninstall -k --user 0 com.google.android.apps.cloudprint
adb shell pm uninstall -k --user 0 com.google.android.syncadapters.contacts #Contact sync
adb shell pm uninstall -k --user 0 com.google.android.tts #Text-to-speech

#Android Apps
adb shell pm uninstall -k --user 0 com.android.calendar
adb shell pm uninstall -k --user 0 com.android.calculator2

# MetroPCS Apps
adb shell pm uninstall -k --user 0 com.handmark.metro.launcher #App Store
adb shell pm uninstall -k --user 0 com.metropcs.metrozone
adb shell pm uninstall -k --user 0 com.ironsrc.aura.tmo #App Selector
adb shell pm uninstall -k --user 0 com.nuance.nmc.sihome.metropcs #myMetro
adb shell pm uninstall -k --user 0 com.privacystar.android.metro #Name ID

# LGE Apps
adb shell pm uninstall -k --user 0 com.lge.email
adb shell pm uninstall -k --user 0 com.lge.music
adb shell pm uninstall -k --user 0 com.lge.voicerecorder
adb shell pm uninstall -k --user 0 com.lge.task
adb shell pm uninstall -k --user 0 com.lge.exchange
adb shell pm uninstall -k --user 0 com.lge.camera
adb shell pm uninstall -k --user 0 com.lge.sizechangable.weather #Default weather app
adb shell pm uninstall -k --user 0 com.lge.sizechangable.weather.platform #Default weather app platform
adb shell pm uninstall -k --user 0 com.lge.sizechangable.weather.theme.optimus #Default weather app theme

# Facebook
adb shell pm uninstall -k --user 0 com.facebook.katana

